include ../partials/mixins.pug

doctype html
html(lang="en")
    head
        meta(charset="utf-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        link(rel="shortcut icon", type="image/x-icon", href= '//whitelabel-prod.herokuapp.com/cdn/v1/' + code + '/favicon.ico')
