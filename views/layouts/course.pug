        include ../partials/two-bar

        script(src="//cdnjs.cloudflare.com/ajax/libs/lodash.js/4.15.0/lodash.js")
        script(src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.1.0/jquery.min.js")
        script(src="/js/main.js")